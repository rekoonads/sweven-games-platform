{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd webrtc-proxy && go build -o ../bin/webrtc-proxy"
  },
  "deploy": {
    "startCommand": "./bin/webrtc-proxy",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
